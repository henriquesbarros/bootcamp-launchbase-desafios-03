{% extends "layout.njk" %}

{% block head %}
<title>Sobre - Rocketseat</title>
{% endblock %}

{% block content %}
    <div id="wrapper">
        <img src="{{ about.images }}" alt="Imagem da {{ about.name }}">
        <h1>{{ about.name }}</h1>
        <p>{{ about.description }}<strong>.</strong></p>
        </ul>
        
        <div class="links-footer">
            {% for link in about.links %}
            <a href="{{ link.url }}" target="_blank">{{ link.name }}</a>
            {% endfor %}
        </div>
    </div>
{% endblock %}
